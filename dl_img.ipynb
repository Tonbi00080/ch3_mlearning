    "import urllib.request as req\n",
    "url = \"http://uta.pw/shodou/img/28/214.png\"\n",
    "# urllib.request.urlretrieveで画像ダウンロード\n",
    "# urlretrieve(目的URL, 保存ファイル名)\n",
    "req.urlretrieve(url, \"test.png\")\n",
    "\n",
    "# OpenCVで読み込む\n",
    "import cv2\n",
    "img = cv2.imread(\"test.png\")\n",
    "# 画素数が出る\n",
    "print(img)"
   ]
